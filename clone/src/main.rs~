fn main() {

    let mut s1 = String::from("Yankho ");
    s1.push_str("Ngolleka");

    println!("Hello, {}", s1);

    let s1 = s2.clone(); //ownership is not moved rather all the heap data is cloned to s2, this an
                         //expensive call
    //we can then access all values from both s1 and s2 since they are pointing to different
    //locations in memory

    println!("Hi, {}", s1);
    println!("Hallo, {}", s2);
}
